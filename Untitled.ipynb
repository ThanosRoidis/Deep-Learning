    "import numpy as np\n",
    "def KL_gaussian(mu_1, s_1, mu_2, s_2):\n",
    "    \n",
    "    return np.log(s_2) - np.log(s_1) + (s_1**2 + (mu_1 - mu_2)**2) / (2* s_2**2) - 0.5"
   ]
  },
